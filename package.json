{
  "name": "jack-maclennan-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier-format": "prettier --config .prettierrc.json 'src/**/*.*' --write",
    "test:vite": "vitest",
    "test:playwright": "playwright test --headed",
    "test:playwright:report": "playwright show-report",
    "document": "typedoc --plugin typedoc-plugin-merge-modules --plugin typedoc-plugin-include-example --plugin typedoc-plugin-inline-sources --plugin typedoc-plugin-github-widget --plugin @zamiell/typedoc-plugin-not-exported",
    "document:watch": "typedoc --plugin typedoc-plugin-merge-modules --plugin typedoc-plugin-include-example --plugin typedoc-plugin-inline-sources --plugin typedoc-plugin-github-widget --plugin @zamiell/typedoc-plugin-not-exported --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "clsx": "^2.1.1",
    "next": "15.1.4",
    "next-themes": "^0.4.4",
    "react": "19.0.0",
    "react-aria-menubutton": "^7.0.3",
    "react-aria-modal": "^5.0.2",
    "react-dom": "19.0.0",
    "react-loader-spinner": "^6.1.6",
    "tailwind-gradient-mask-image": "^1.2.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.6",
    "@types/react": "19.0.7",
    "@types/react-aria-modal": "^5.0.0",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@zamiell/typedoc-plugin-not-exported": "^0.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "typedoc": "^0.27.6",
    "typedoc-plugin-github-widget": "^1.0.0",
    "typedoc-plugin-include-example": "^2.0.2",
    "typedoc-plugin-inline-sources": "^1.2.0",
    "typedoc-plugin-merge-modules": "^6.1.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.7",
      "@types/react-dom": "19.0.3"
    }
  }
}
