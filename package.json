{
  "name": "jack-maclennan-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier-format": "prettier --config .prettierrc.json 'src/**/*.*' --write",
    "test:vite": "vitest",
    "test:playwright": "playwright test --headed",
    "test:playwright:report": "playwright show-report",
    "document": "typedoc --plugin typedoc-plugin-merge-modules --plugin typedoc-plugin-include-example --plugin typedoc-plugin-inline-sources --plugin typedoc-plugin-github-widget --plugin @zamiell/typedoc-plugin-not-exported",
    "document:watch": "typedoc --plugin typedoc-plugin-merge-modules --plugin typedoc-plugin-include-example --plugin typedoc-plugin-inline-sources --plugin typedoc-plugin-github-widget --plugin @zamiell/typedoc-plugin-not-exported --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "clsx": "^2.1.1",
    "next": "^14.2.12",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-aria-menubutton": "^7.0.3",
    "react-aria-modal": "^5.0.2",
    "react-dom": "^18",
    "react-loader-spinner": "^6.1.6",
    "tailwind-gradient-mask-image": "^1.2.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@playwright/test": "1.48.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-aria-modal": "^4.0.10",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.3",
    "@zamiell/typedoc-plugin-not-exported": "^0.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-playwright": "^1.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^25.0.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typedoc": "^0.26.11",
    "typedoc-plugin-github-widget": "^1.0.0",
    "typedoc-plugin-include-example": "^1.4.0",
    "typedoc-plugin-inline-sources": "^1.1.0",
    "typedoc-plugin-merge-modules": "^6.0.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  }
